    st_set_geometry(NULL) %>%
    filter(minstay<30,licence_status_verified=="Issued",room_type %in% c("Entire home/apt")) %>%
    group_by(Date,host_id) %>%
    summarize(multiple_count=n()-1) %>%
    filter(multiple_count>0) %>%
    mutate(Series="Per host"),
