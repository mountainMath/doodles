ggplot(minstay_data,aes(x=minstay_d,y=Count,fill=room_type))+
  geom_bar(stat="identity") +
  my_theme +
  scale_y_continuous(labels=scales::comma) +
  scale_fill_manual(values=room_type_colors,na.value="grey") +
